{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es2015",
    "module": "amd",
    "jsx": "react",
    "outFile": "index.js",
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
